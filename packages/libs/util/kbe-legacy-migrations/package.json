{
  "name": "@abx-utils/kbe-legacy-migrations",
  "version": "1.1.0",
  "description": "Contains the legacy db-migrations taken from kinesis-secondary-exchange-repository",
  "license": "ISC",
  "main": "dist/index",
  "types": "src/index",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "tsc -p tsconfig.build.json --skipLibCheck",
    "run-legacy-migrations-test": "npm run compile && NODE_ENV=test NODE_PATH=dist node dist/runner.js",
    "run-legacy-migrations-dev": "npm run compile && NODE_ENV=dev NODE_PATH=dist node dist/runner.js"
  },
  "publishConfig": {
    "access": "restricted"
  },
  "gitHead": "8a29dce4e9bfe143b1c40c7196d4b51ea0d72d8a",
  "dependencies": {
    "@abx-utils/db-connection-utils": "^1.1.0",
    "node-uuid": "1.4.7",
    "sequelize": "4.41.0",
    "validator": "10.2.0"
  },
  "devDependencies": {
    "@lerna/filter-options": "^3.18.4",
    "@types/chai": "4.0.3",
    "@types/chai-as-promised": "7.1.0",
    "@types/mocha": "2.2.41",
    "@types/node": "12.12.11",
    "@types/sequelize": "3.30.4",
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "mocha": "5.2.0",
    "source-map-support": "0.5.9",
    "ts-mocha": "6.0.0",
    "ts-node": "8.5.2",
    "ts-node-dev": "1.0.0-pre.44",
    "tslint": "5.20.1",
    "typescript": "3.7.2"
  }
