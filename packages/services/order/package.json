{
  "name": "@abx/order-service",
  "version": "1.0.3",
  "description": "> TODO: description",
  "main": "dist/index",
  "types": "src/index",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "swagger-gen": "tsoa swagger ",
    "routes-gen": "tsoa routes",
    "build-order-data": "npm run swagger-gen -- -c ./src/service-wrappers/order-data/tsoa.json && npm run routes-gen -- -c ./src/service-wrappers/order-data/tsoa.json",
    "build-order-gateway": "npm run swagger-gen -- -c ./src/service-wrappers/order-gateway/tsoa.json && npm run routes-gen -- -c ./src/service-wrappers/order-gateway/tsoa.json",
    "build": "npm run clean && npm run compile",
    "clean": "rm -rf ./dist",
    "compile": "npm run build-order-data && npm run build-order-gateway && tsc -p tsconfig.build.json --skipLibCheck",
    "migrate-db": "npm run build && NEW_RELIC_NO_CONFIG_FILE=true NODE_ENV=test NODE_PATH=dist node dist/migrations/index.js",
    "start-local:worker": "NODE_ENV=dev ts-node-dev src/service-wrappers/worker/bootstrap.ts > ../../../_logs/worker.logs",
    "start-local:order-data": "NODE_ENV=dev ts-node-dev src/service-wrappers/order-data/bootstrap.ts > ../../../_logs/order-data.logs",
    "start-local:order-gateway": "NODE_ENV=dev ts-node-dev src/service-wrappers/order-gateway/bootstrap.ts > ../../../_logs/order-gateway.logs",
    "start-local:settlement": "NODE_ENV=dev ts-node-dev src/service-wrappers/settlement/bootstrap.ts > ../../../_logs/settlement.logs",
    "test": "NEW_RELIC_NO_CONFIG_FILE=true NODE_PATH=dist ts-mocha -p tsconfig.build.json --opts ../../../../_test-setup/mocha.opts --require ../../../_test-setup/test_globals.js src/**/*.spec.ts --exit",
    "build-image-from-tag": "echo build image here using $VERSION env variable, set by travis",
    "release-image-integration": "echo push image to ECR here",
    "release-image-uat": "echo push image to ECR here",
    "update-service-integration": "echo updating integration service here"
  },
